{
  "name": "flux-pack-tutorial",
  "version": "0.0.0",
  "description": "Hands on flux-pack to drive you through the creation of a flux-pack",
  "author": "",
  "keywords": [],
  "main": "dist/{{name}}.js",
  "types": "src/index.ts",
  "scripts": {
    "doc": "typedoc src/ --exclude src/index.ts --out dist/docs --theme default",
    "build": "yarn build:dev",
    "build:dev": "python scripts/auto-gen.py && webpack --mode development && yarn doc",
    "build:prod": "python scripts/auto-gen.py && webpack --mode production && yarn doc",
    "test": "jest -c ./jest.config.js --rootDir .",
    "test-coverage": "jest -c ./jest.config.js --rootDir . --collect-coverage --collectCoverageFrom=./src/lib/**/*.ts "
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@youwol/flux-view": "0.x",
    "@youwol/flux-lib-core": "1.x"
  },
  "devDependencies": {
    "typedoc":"^0.20.32",
    "rxjs": "6.x",
    "reflect-metadata": "0.1.13",
    "@youwol/flux-view": "0.x",
    "@youwol/flux-lib-core": "1.x",
    "@types/node": "14.14.16",
    "awesome-typescript-loader": "5.2.1",
    "eslint": "7.16.0",
    "eslint-loader": "4.0.2",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3",
    "webpack": "^4.12.0",
    "webpack-cli": "4.3.0"
  },
  "youwol": {
    "type": "flux-pack",
    "pipeline": {
      "name": "yw_pipeline_flux_pack_tutorial",
      "version": "0.0.0"
    }
  }
}