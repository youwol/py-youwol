{
    "name": "@youwol/flux-builder",
    "version": "0.0.9",
    "description": "Front end side of the Flux builder application",
    "author": "greinisch@youwol.com",
    "main": "dist/index.html",
    "scripts": {
        "build": "yarn build:dev",
        "build:dev": "del-cli dist && webpack --mode development",
        "build:prod": "del-cli dist && webpack --mode production",
        "test": "jest -c ./jest.config.js --rootDir . -i",
        "test-coverage": "jest -c ./jest.config.js --rootDir . -i --collect-coverage --collectCoverageFrom=./src/app/**/*.ts ",
        "doc": "typedoc src/ --exclude src/index.ts --exclude src/tests --out dist/docs --theme default",
        "start": "webpack serve",
        "upgrade-youwol": "yarn upgrade @youwol/cdn-client @youwol/flux-core @youwol/flux-svg-plots @youwol/flux-view @youwol/fv-button @youwol/fv-context-menu @youwol/fv-group @youwol/fv-input @youwol/fv-tabs @youwol/fv-tree",
        "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public"
    },
    "license": "MIT",
    "dependencies": {
        "@youwol/cdn-client": "0.x",
        "@youwol/flux-core": "0.x",
        "@youwol/flux-svg-plots": "0.x",
        "@youwol/flux-view": "0.x",
        "@youwol/fv-button": "0.x",
        "@youwol/fv-context-menu": "0.x",
        "@youwol/fv-group": "0.x",
        "@youwol/fv-input": "0.x",
        "@youwol/fv-tabs": "0.x",
        "@youwol/fv-tree": "0.x",
        "d3-dispatch": "2.x",
        "d3-drag": "2.x",
        "d3-scale": "3.x",
        "d3-selection": "2.x",
        "d3-zoom": "2.x",
        "grapesjs": "0.x",
        "lodash": "4.x",
        "reflect-metadata": "0.x",
        "rxjs": "6.x"
    },
    "devDependencies": {
        "@types/jest": "26.0.22",
        "@types/node": "14.14.37",
        "css-loader": "5.2.0",
        "del-cli": "3.0.1",
        "eslint": "7.23.0",
        "eslint-webpack-plugin": "2.5.3",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.2.0",
        "jest": "26.6.3",
        "jest-junit": "12.0.0",
        "mini-css-extract-plugin": "1.3.9",
        "source-map-loader": "2.0.1",
        "ts-jest": "26.5.4",
        "ts-lib": "0.0.5",
        "ts-loader": "8.1.0",
        "typedoc": "0.20.34",
        "typescript": "4.2.3",
        "webpack": "5.28.0",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "4.6.0",
        "webpack-dev-server": "3.11.2"
    },
    "youwol": {
        "type": "application",
        "pipeline": {
            "name": "yw_pipeline_webpack_ts",
            "version": "0.0.0"
        },
        "cdnDependencies": {
            "@youwol/cdn-client": "0.x",
            "@youwol/flux-core": "0.x",
            "@youwol/flux-svg-plots": "0.x",
            "@youwol/flux-view": "0.x",
            "@youwol/fv-button": "0.x",
            "@youwol/fv-context-menu": "0.x",
            "@youwol/fv-group": "0.x",
            "@youwol/fv-input": "0.x",
            "@youwol/fv-tabs": "0.x",
            "@youwol/fv-tree": "0.x",
            "codemirror": "5.x",
            "grapesjs": "0.x",
            "lodash": "4.x",
            "reflect-metadata": "0.x",
            "rxjs": "6.x"
        }
    }
}
